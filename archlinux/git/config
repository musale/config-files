[user]
    name = Martin Musale
    email = martinmshale@gmail.com

[alias]
    a = add -p
    all = add --all
    c = commit -m
    st = status
    co = checkout
    up = pull --rebase=preserve
    g = grep --break --heading --line-number
    fa = fetch --all
    p = push
    pf = push -f

[color]
    diff = auto
    status = auto
    branch = auto

[push]
    default = simple

[merge]
    tool = vimdiff

[mergetool]
    keepBackup = false

